  "private": true,
  "scripts": {
    "dev": "webpack serve --config ./webpack.dev.js",
    "build": "rm -rf ./dist && webpack --config ./webpack.prod.js --progress",
    "lint:eslint": "eslint --ext .ts,.tsx \"src/**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --list-different \"src/**/*.{json,html}\"",
    "lint:type": "tsc --pretty",
    "lint:stylelint": "stylelint \"**/*.scss\"",
    "lint": "FORCE_COLOR=1 npm-run-all -p --print-label \"lint:**\"",
    "clean": "rm -rf ./node_modules ./dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.27.5",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "stylelint": "^15.6.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "jotai": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.0"
  }
